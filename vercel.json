{
    "redirects": [
        {
            "source": "/landing",
            "destination": "/",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/app",
            "destination": "/app.html"
        },
        {
            "source": "/pages/:path*",
            "destination": "/pages/:path*.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}